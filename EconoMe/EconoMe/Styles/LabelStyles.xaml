<?xml version="1.0" encoding="utf-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!--Gray-->
    <Style x:Key="MediumGrayLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="TextColor" Value="{StaticResource Gray}"/>
        <Setter Property="FontSize" Value="{StaticResource MediumSize}"/>
    </Style>

    <Style x:Key="MediumBlueLabelStyle" TargetType="{x:Type Label}">
        <Setter Property="TextColor" Value="{StaticResource Blue}"/>
        <Setter Property="FontSize" Value="{StaticResource MediumSize}"/>
    </Style>

</ResourceDictionary>